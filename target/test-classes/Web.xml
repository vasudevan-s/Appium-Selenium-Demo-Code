<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!-- Created By: Vasudevan Sampath -->

<suite name="App Suite" parallel="false" thread-count="1" verbose="10" >
    <test name="Smoke" enabled="true">
        <parameter name="browserName" value="msedge"/>
        <parameter name="launchURL" value="http://www.apple.com"/>
        <classes>
            <class name="org.example.appname.suites.smoke.WebTests"/>
        </classes>
    </test>

    <test name="Regression" enabled="false">
        <parameter name="browserName" value="firefox"/>
        <parameter name="launchURL" value="http://www.apple.com"/>
        <classes>
            <class name="org.example.appname.suites.smoke.WebTests"/>
        </classes>
    </test>
</suite>